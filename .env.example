# Environment variables for JonBuhTrader

# Server Configuration
PORT=8080

# Database Configuration
DATABASE_URL=postgres://user:password@localhost:5432/jonbuhtrader?sslmode=disable

# API Keys (replace with actual values)
BROKER_API_KEY=your_broker_api_key_here
BROKER_API_SECRET=your_broker_api_secret_here
MARKET_DATA_API=your_market_data_api_key_here

# Trading Configuration
MAX_POSITION_SIZE=10000.0
RISK_PER_TRADE=0.02
ENABLE_PAPER_TRADE=true

# Logging
LOG_LEVEL=info
